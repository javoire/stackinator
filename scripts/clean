#!/usr/bin/env bash
set -e

# Clean script for stackinator

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
PROJECT_ROOT="$(dirname "$SCRIPT_DIR")"
cd "$PROJECT_ROOT"

echo "Cleaning build artifacts..."

if [ -f "stack" ]; then
  rm stack
  echo "✓ Removed ./stack"
fi

go clean

echo "✓ Clean complete"

